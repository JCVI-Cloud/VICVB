<tool id="vicvb_vigor_tgz_to_jbrowse_converter" name="Convert VIGOR tarball output to JBrowse data directory" version="1.0">
  <command interpreter="python">
   vicvb_vigor_tgz_to_jbrowse_converter.py '$inp_vigor_tgz'
   '$out_vcb_jbrowse' '${out_vcb_jbrowse.extra_files_path}'
  </command>
  <inputs>
    <param format="vigor.tgz" name="inp_vigor_tgz" type="data" label="VIGOR output archive file"/>
  </inputs>
  <outputs>
    <data format="vicvb.jbrowse" name="out_vcb_jbrowse" metadata_source="inp_vigor_tgz"/>
  </outputs>
  <help>
  </help>
</tool>
